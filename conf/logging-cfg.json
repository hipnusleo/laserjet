{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "simple",
            "stream": "ext://sys.stdout"
        },
        "info_file_handler": {
            "level": "DEBUG",
            "class": "cloghandler.ConcurrentRotatingFileHandler",
            "maxBytes": 10485760,
            "backupCount": 50,
            "delay": true,
            "filename": "../../logs/laserjet.log",
            "formatter": "verbose"
        }
    },
    "loggers": {
        "laserjet": {
            "level": "ERROR",
            "handlers": [
                "console"
            ],
            "propagate": "no"
        }
    },
    "root": {
        "level": "INFO",
        "handlers": [
            "console",
            "info_file_handler",
            "error_file_handler"
        ]
    }
}